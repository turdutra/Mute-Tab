{
  "manifest_version": 2,
  "name": "Mute tabs",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "flag.png"
  },
  "permissions": [
      "tabs",
      "activeTab"
  ]
}
